{
  "name": "ng-infinite-autocomplete",
  "version": "2.0.5",
  "description": "AngularJS 1.x infinite-autocomplete wrapper",
  "main": "./dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run build:umd && npm run build:umd:debug",
    "watch": "npm run clean && npm run build:watch",
    "build:watch": "webpack ./src/index.ts ./dist/index.debug.js --output-library-target=\"umd\" -d --watch",
    "build:umd": "webpack ./src/index.ts ./dist/index.js --output-library-target=\"umd\" -p --config webpack.production.config.js",
    "build:umd:debug": "webpack ./src/index.ts ./dist/index.debug.js --output-library-target=\"umd\" -d",
    "prepublish": "npm run build",
    "test": "karma start karma.conf.js",
    "test:watch": "karma start karma.conf.js --single-run false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Attrash-Islam/ng-infinite-autocomplete.git"
  },
  "keywords": [
    "autocomplete",
    "infinite",
    "infinite",
    "autocomplete",
    "infinite-autocomplete",
    "angular",
    "angularjs",
    "component",
    "web"
  ],
  "author": "Islam Attrash",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Attrash-Islam/ng-infinite-autocomplete/issues"
  },
  "homepage": "https://github.com/Attrash-Islam/ng-infinite-autocomplete#readme",
  "devDependencies": {
    "@types/angular": "1.6.1",
    "@types/angular-mocks": "1.5.8",
    "@types/jasmine": "2.5.40",
    "@types/jquery": "2.0.39",
    "angular": "1.6.1",
    "angular-mocks": "1.6.1",
    "coveralls": "2.11.15",
    "istanbul-instrumenter-loader": "1.2.0",
    "jasmine": "2.5.3",
    "karma": "1.3.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-source-map-support": "1.2.0",
    "karma-webpack": "2.0.1",
    "rimraf": "2.5.4",
    "ts-loader": "1.3.3",
    "typescript": "2.1.5",
    "webpack": "1.14.0"
  },
  "dependencies": {
    "infinite-autocomplete": "2.2.0"
  }
}
